<%- include("partials/header") -%>

<h1>Home</h1>
<p><%= homeStartingContent %></p>

<div class="container">
    <% posts.forEach(function(post) { %>
        <h1><%= post["postTitle"] %></h1>
        <p>
            <%= post["postBody"].substring(0,120) + "..." %>
            <a href="/posts/<%= post["postTitle"] %>" >Read More</a>
        </p>
    <% }); %>
</div>

<%- include("partials/footer") -%>